{
  "extends": "airbnb-base",
  "plugins": [
    "import"
  ],
  "globals": {
    "App": true,
    "Page": true
  },
  "rules": {
    "comma-dangle": ["error", "never"],
    "no-console": 0,
    "no-param-reassign": 0,
    "consistent-return": 0,
    "no-plusplus": 0,
    "no-unused-vars": 0,
    "arrow-body-style": 0,
    // disallow certain syntax forms
    // http://eslint.org/docs/rules/no-restricted-syntax
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ]
  }
}
